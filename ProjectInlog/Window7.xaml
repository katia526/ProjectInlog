<Window x:Class="ProjectInlog.Window7"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectInlog"
        mc:Ignorable="d"
        Title="Window7" Height="450" Width="800">
    <Grid>
        <StackPanel Width="400" Height="300" Margin="10">
            <ComboBox x:Name="cbLeverancier" Width="300" Height="30" Margin="0,20"
                ItemsSource="{Binding col}" 
                      DisplayMemberPath="Name"
                     SelectedValuePath="Id"
                      SelectedValue="{Binding col}">
               
            </ComboBox>
            <Button x:Name="btnDelete" Content="Delete" Width="100" Height="20" Margin="0,100" Click="btnDelete_Click"></Button>
        </StackPanel>
        
    </Grid>
</Window>
